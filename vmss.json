{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachineScaleSets_testscale_name": {
            "defaultValue": "testscale",
            "type": "String"
        },
        "virtualNetworks_dataGenSwarm_Vnet_001_externalid": {
            "defaultValue": "/subscriptions/1ac6fdb8-61a9-4e86-a871-1baff37cd9e3/resourceGroups/Miztiik_Enterprises_scale_vmss_on_events_001/providers/Microsoft.Network/virtualNetworks/dataGenSwarm_Vnet_001",
            "type": "String"
        },
        "loadBalancers_lb_externalid": {
            "defaultValue": "/subscriptions/1ac6fdb8-61a9-4e86-a871-1baff37cd9e3/resourceGroups/Miztiik_Enterprises_scale_vmss_on_events_001/providers/Microsoft.Network/loadBalancers/lb",
            "type": "String"
        },
        "networkSecurityGroups_basicNsgdataGenSwarm_Vnet_001_nic01_externalid": {
            "defaultValue": "/subscriptions/1ac6fdb8-61a9-4e86-a871-1baff37cd9e3/resourceGroups/Miztiik_Enterprises_scale_vmss_on_events_001/providers/Microsoft.Network/networkSecurityGroups/basicNsgdataGenSwarm_Vnet_001-nic01",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Compute/virtualMachineScaleSets",
            "apiVersion": "2022-11-01",
            "name": "[parameters('virtualMachineScaleSets_testscale_name')]",
            "location": "westeurope",
            "sku": {
                "name": "Standard_D2lds_v5",
                "tier": "Standard",
                "capacity": 1
            },
            "zones": [
                "1",
                "2",
                "3"
